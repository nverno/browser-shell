{
  "name": "browser-shell",
  "version": "0.0.1",
  "description": "Chrome extension to run a shell in browser",
  "license": "GPL-3.0-or-later",
  "author": "Noah Peart <noah.v.peart@gmail.com> (https://www.github.com/nverno)",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build --mode development",
    "build:lodash": "lodash include=chain",
    "build:production": "tsc && vite build",
    "build:tsc": "tsc && tsc-alias",
    "dev": "vite --host",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "start": "ts-node ./src/index.ts"
  },
  "dependencies": {
    "browser-fs-access": "^0.35.0",
    "debug": "^4.4.0",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.23",
    "@eslint/js": "^9.24.0",
    "@types/debug": "^4.1.12",
    "@types/eslint__js": "^9.14.0",
    "@types/jquery": "^3.5.32",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.1",
    "autoprefixer": "^10.4.21",
    "chrome-types": "^0.1.347",
    "eslint": "^9.24.0",
    "globals": "^16.0.0",
    "lodash-cli": "^4.17.5",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^7.0.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.15",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.6",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
